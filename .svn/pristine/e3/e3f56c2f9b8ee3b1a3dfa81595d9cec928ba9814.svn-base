import request from '@/utils/request';
import { stringify } from 'qs';

export async function addImage(params) {
  return request(`/lazy-card/appImage/saveOrUpdateAppImage`, {
    method: 'POST',
    data: params,
  });
}
export async function queryImages(params) {
  return request(`/lazy-card/appImage/appImageList`, {
    method: 'POST',
    data: params,
  });
}
