import request from '@/utils/request';
import { stringify } from 'qs';

export async function querySalesman(params) {
  return request('/lazy-card/agent/salesmanList',{
    method: 'POST',
    data:params
  });
};

export async function saveOrUpdateSalesman(params) {
  return request('/lazy-card/agent/saveOrUpdateSalesman',{
    method: 'POST',
    data: params,
  });
}

export async function prohibitSalesmanById(params) {
  const id = encodeURIComponent(params);
  return request(`/lazy-card/agent/prohibitSalesman?id=${id}`);
}

export async function startUpSalesmanById(params) {
  const id = encodeURIComponent(params);
  return request(`/lazy-card/agent/startUpSalesman?id=${id}`);
}

