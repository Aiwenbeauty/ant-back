import request from '@/utils/request';
import { stringify } from 'qs';

export async function queryCode(params) {
  return request('/lazy-card/invitationCode/queryCode', {
    method: 'GET',
    // data:params
  });
};

export async function getMerchInfoRAS(params) {
  return request('/lazy-card/merchInfo/getMerchInfoRAS', {
    method: 'GET',
    // data:params
  });
};

export async function updateMerchInfoRAS(params) {
  return request('/lazy-card/merchInfo/updateMerchInfoRAS', {
    method: 'POST',
    data: params,
  });
}
export async function againMerchInfoRAS(params) {
  return request('/lazy-card/merchInfo/againMerchInfoRAS', {
    method: 'POST',
    data: params,
  });
}
export async function downloadRAS(params) {
  const merchNo = encodeURIComponent(params);
  return request(`/lazy-card/merchInfo/downloadRAS?merchNo=${merchNo}`);
}
