import request from '@/utils/request';

/**
 * 编辑 or 新增
 * @param {*} params 
 */
export async function saveOrUpedata(params) {
  return request('/lazy-card/payChannel/saveOrUpedata',{
    method: 'POST',
    data:params
  });
}

