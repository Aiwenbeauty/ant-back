@import '~antd/lib/style/themes/default.less';
@import '~@/utils/utils.less';
.test_col {
    background: red;

    :global {
        background: green;
    }
}

.listContent {
    font-size: 0;
    .listContentItem {
      display: inline-block;
      margin-left: 40px;
      color: @text-color-secondary;
      font-size: @font-size-base;
      vertical-align: middle;
      > span {
        line-height: 20px;
      }
      > p {
        margin-top: 4px;
        margin-bottom: 0;
        line-height: 22px;
      }
    }
  }

:global(.slick-slide) {
    text-align : center;
    height     : 160px;
    line-height: 160px;
    background : #364d79;
    overflow   : hidden;
}


:global(.ant-row .ant-carousel .slick-slide h1) {
    color: #fff;
}