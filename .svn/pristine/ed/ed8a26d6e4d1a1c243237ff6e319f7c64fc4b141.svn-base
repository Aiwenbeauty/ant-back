import request from '@/utils/request';

/**
 * 编辑 or 新增
 * @param {*} params 
 */
export async function saveOrUpedata(params) {
  return request('/lazy-card/newbie/saveOrUpedata',{
    method: 'POST',
    data:params
  });
}

