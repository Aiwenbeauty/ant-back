import request from '@/utils/request';
import { stringify } from 'qs';

export async function memberAddressListInfo(params) {
  return request('/lazy-card/memberAddress/memberAddressList',{
    method: 'POST',
    data:params
  });
};
export async function areaListInfo(params) {
    const pid = encodeURIComponent(params);
    return request(`/lazy-card/area/areaList?pid=${pid}`);
  };

