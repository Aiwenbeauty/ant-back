/* eslint react/no-multi-comp:0 */
@connect(({ statistics, loading }) => ({
    statistics,
  loading: loading.models.rule,
}))

@Form.create()
class CartItemList extends PureComponent {
  constructor(props) {
    super(props)
    this.state = {
      modalVisible: false,
      currentRecord: null,
      pageSize: 10,
    }
  }

  componentDidMount() {
    const { dispatch } = this.props;
    const { pageSize } = this.state;

    dispatch({
      type: 'statistics/agentDimension',
      payload:{
      }
    }).then(()=>{
        const {statistics} = this.props;
        this.dataInfo(statistics);
    });
  }

  // 组装数据
  dataInfo(statistics){
    // 清空数组
    time.splice(0,time.length); 
    countryNum.splice(0,countryNum.length); 
    provinceNum.splice(0,provinceNum.length); 
    cityNum.splice(0,cityNum.length); 
    

    const agentStatisticsList = statistics.agentStatisticsList.data;
    agentStatisticsList.forEach(item => {
        time.push(item.agentime);
        countryNum.push(item.countryNum);
        provinceNum.push(item.provinceNum);
        cityNum.push(item.cityNum);
    
  
    })
    const myChart = echarts.init(document.getElementById('main'));
    myChart.setOption(optionData);
  }

  // 查询
  handleSearch = (e) => {
    const { dispatch, form } = this.props;
    const { pageSize } = this.state;
    e.preventDefault();
    form.validateFields((err, fieldsValue) => {
      if (!err) {
        // console.log(fieldsValue)
          const value={
            startTime: fieldsValue.time[0] != null ? fieldsValue.time[0].format('YYYY-MM-DD') : null,
            endTime: fieldsValue.time[1] != null ? fieldsValue.time[1].format('YYYY-MM-DD') : null,
          }
        dispatch({
          type: 'statistics/agentDimension',
          payload:{
            ...value 
          }
        }).then(()=>{
            const {statistics} = this.props;
            this.dataInfo(statistics);
        });
      }
    })
  }

 // 重置
  handleFormReset = () => {
    const { form, dispatch } = this.props;
    form.resetFields();
    dispatch({
      type: 'statistics/agentDimension',
      payload:{
      }
    }).then(()=>{
        const {statistics} = this.props;
        this.dataInfo(statistics);
    });
  };

  renderForm = () => {
    const { getFieldDecorator } = this.props.form;
    const { form} = this.props;
    const FormItem = Form.Item;
    return (
      <Form labelCol={{ span:4, md: 4 ,lg:8}} wrapperCol={{ span:19,md: 19,lg:16 }} onSubmit={this.handleSearch}>
        <Row>
          <Col span={24} md={24} lg={8}>
            <FormItem label="日期">
              {getFieldDecorator('time',
                {
                  rules: []
                }
              )(<RangePicker />)}
            </FormItem>
          </Col>
          <Col span={24} md={24} lg={8} offset={16}>
            <FormItem className={styles.btnContainer}>
              <Button type="primary" htmlType="submit">
                查询
              </Button>
              <Button style={{marginLeft:20}} onClick={this.handleFormReset}>
                重置
              </Button>
            </FormItem>
          </Col>
        </Row>
      </Form>
    )
  }

  onTabChange = (key, type) => {
    this.setState({ [type]: key });
  };

  render() {
    const {statistics} = this.props;
    const agentStatisticsList = statistics.agentStatisticsList.data; 
    const columns = [
      {
        title: '时间',
        dataIndex: 'agentime',
        key: 'agentime',
      },
      {
        title: '国代数',
        dataIndex: 'countryNum',
        key: 'countryNum',
      },
      {
        title: '省代数',
        dataIndex: 'provinceNum',
        key: 'provinceNum',
      },
      {
        title: '市代数',
        dataIndex: 'cityNum',
        key: 'cityNum',
      },
    ]
    //设置默认分页(表格自带分页)
    const pagination ={
          pageSize:10
        }
    return (
      <PageHeaderWrapper title="代理商维度">
        <Card>
          <Row>
            {this.renderForm()}
          </Row>
          <div id="main" style={{ width: '100%', height: 500 }} />
        </Card>
        <Card>
          <Table
                dataSource={agentStatisticsList}
                rowKey="index"
                className="textCenter"
                rowClassName="textCenter"
                columns={columns}
                bordered={true}
                pagination={pagination}
                scroll={{ x: 'max-content' }}
                onChange={this.handlePageChange}
                size="small"
              />
        </Card>
      </PageHeaderWrapper>
    )
  }
}
export default CartItemList;