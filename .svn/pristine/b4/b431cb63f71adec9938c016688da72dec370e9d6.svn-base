import request from '@/utils/request';

export async function query(params) {
    return request('/lazy-card/message/messageList', {
        method: 'POST',
        data: params
    });
}

export async function saveOrUpdateNews(params) {
    return request('/lazy-card/message/saveOrUpedata', {
        method: 'POST',
        data: params,
    });
}
export async function deleteNews(params) {
    return request('/lazy-card/message/deleteById', {
        method: 'POST',
        data: params
    });
}

